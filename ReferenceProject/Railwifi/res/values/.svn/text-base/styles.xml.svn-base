<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light.NoTitleBar">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    
    
  <style name="CalendarTitle">
    <item name="android:textSize">@dimen/calendar_text_medium</item>
    <item name="android:textStyle">bold</item>
    <item name="android:background">@drawable/calendar_title_bg</item>
    <item name="android:textColor">#ffffff</item>
    <item name="android:paddingLeft">22.5dp</item>	<item name="android:paddingRight">22.5dp</item>
  </style>

  <style name="CalendarCell">
    <item name="android:layout_width">0dp</item>
    <item name="android:layout_weight">1</item>
    <item name="android:gravity">top|left</item>
    <item name="android:includeFontPadding">false</item>
    <item name="android:paddingLeft">3dp</item>
    <!-- <item name="android:textAlignment">center</item> -->
  </style>

  <style name="CalendarCell.DayHeader">
    <item name="android:textSize">@dimen/calendar_text_small</item>
    <item name="android:textColor">#ffffff</item>
    <item name="android:gravity">center_vertical</item>
    <item name="android:paddingBottom">4.5dp</item>
    <item name="android:paddingTop">4.5dp</item>
  </style>

  <style name="CalendarCell.CalendarDate">
    <item name="android:textSize">@dimen/calendar_text_medium</item>
    <item name="android:clickable">true</item>
    <item name="android:textStyle">bold</item>
  </style>
  
  <style name="CmmobiDialog" parent="@android:style/Theme.Dialog">
     <item name="android:windowFrame">@null</item> <!--Dialog的windowFrame框为无 --> 
     <item name="android:windowIsFloating">true</item><!-- 是否漂现在activity上 -->
     <item name="android:windowIsTranslucent">true</item><!-- 是否半透明 -->
     <item name="android:windowNoTitle">true</item>
     <item name="android:backgroundDimEnabled">true</item>
     <item name="android:windowBackground">@android:color/transparent</item>
     <item name="android:maxHeight">329dp</item>
   </style>
   
<!--        <item name="android:paddingBottom">4.5dp</item>
     <item name="android:paddingTop">4.5dp</item> -->
   
   <style name="CustomRatingBar" parent="@android:style/Widget.RatingBar"> 
		<item name="android:progressDrawable">@drawable/star_bg</item> 
		<item name="android:minHeight">21dp</item> 
		<item name="android:maxHeight">21dp</item> 
    </style>
    
   <style name="Theme.SmoothProgressBarDefaults" parent="android:Theme">
        <item name="spbStyle">@style/SmoothProgressBar</item>
    </style>

    <style name="SmoothProgressBar" parent="android:Widget.ProgressBar.Horizontal">
        <item name="spb_color">@color/spb_default_color</item>
        <item name="spb_sections_count">@integer/spb_default_sections_count</item>
        <item name="spb_speed">@string/spb_default_speed</item>
        <item name="spb_stroke_width">@dimen/spb_default_stroke_width</item>
        <item name="spb_stroke_separator_length">@dimen/spb_default_stroke_separator_length</item>
        <item name="spb_reversed">@bool/spb_default_reversed</item>
        <item name="spb_mirror_mode">@bool/spb_default_mirror_mode</item>
        <item name="spb_interpolator">@integer/spb_default_interpolator</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:maxHeight">48dp</item>
    </style>
    <style name="SohuProgressSmall" parent="@android:attr/progressBarStyleSmall">
        <item name="android:layout_width">21.0dip</item>
        <item name="android:layout_height">21.0dip</item>
        <item name="android:indeterminate">true</item>
        <item name="android:indeterminateDrawable">@drawable/progressbar_foxtail_small</item>
    </style>
    <style name="MediaControllerTopBar">
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@color/player_vip_color</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">33.0dip</item>
        <item name="android:layout_alignParentTop">true</item>
    </style>
    <style name="MediaControllerTopTitleText">
        <item name="android:textSize">@dimen/text_size_medium</item>
        <item name="android:textColor">@color/player_text_color</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">3.0dip</item>
        <item name="android:layout_marginRight">7.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1.0</item>
    </style>
</resources>
